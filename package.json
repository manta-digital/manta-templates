{
    "name": "manta-templates",
    "version": "0.8.0",
    "private": true,
    "scripts": {
        "update-guides": "bash project-documents/ai-project-guide/scripts/template-stubs/update-guides.sh",
        "sync-template": "node scripts/sync-template.js",
        "copy-packages": "node scripts/copy-packages.js",
        "copy-packages:nextjs": "node scripts/copy-packages.js nextjs",
        "copy-packages:react": "node scripts/copy-packages.js react",
        "copy-packages:electron": "node scripts/copy-packages.js electron",
        "copy-packages:all": "node scripts/copy-packages.js nextjs && node scripts/copy-packages.js react && node scripts/copy-packages.js electron",
        "dev:template": "pnpm --filter \"./templates/nextjs\" dev",
        "dev:landing": "pnpm --filter manta-landing dev",
        "build:template": "pnpm --filter \"./templates/nextjs\" build",
        "build:landing": "pnpm --filter manta-landing build",
        "build": "pnpm -r build",
        "code-review": "node scripts/code-review-agent.js",
        "code-review:quick": "node scripts/code-review-agent.js --depth=quick",
        "code-review:deep": "node scripts/code-review-agent.js --depth=deep"
    },
    "workspaces": [
        "templates/*",
        "packages/*",
        "packages/ui-adapters/*",
        "landing"
    ],
    "description": "This template ships with a curated set of **public** guides. To pull them into your project:",
    "main": "index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/manta-digital/manta-templates.git"
    },
    "keywords": [],
    "pnpm": {
        "overrides": {
            "brace-expansion": "^2.0.2",
            "form-data": "4.0.4"
        },
        "onlyBuiltDependencies": [
            "@parcel/watcher",
            "@tailwindcss/oxide",
            "esbuild",
            "sharp",
            "unrs-resolver"
        ],
        "ignoredBuiltDependencies": [
            "chromedriver",
            "electron",
            "electron-winstaller"
        ]
    },
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "bugs": {
        "url": "https://github.com/manta-digital/manta-templates/issues"
    },
    "homepage": "https://github.com/manta-digital/manta-templates#readme"
}
